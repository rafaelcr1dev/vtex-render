{
  "name": "NAMESTORE",
  "storeNameToRenamesFiles": "NAMESTORE",
  "color": "#1976d2",
  "version": "0.1.1",
  "description": "Starter for HTML projects.",
  "engines": {
    "node": ">=6.9.1"
  },
  "framework": "react",
  "scripts": {
    "sprites": "gulp sprites --gulpfile config/gulpfile.js",
    "svgs": "gulp svgs --gulpfile config/gulpfile.js",
    "start": "gulp --gulpfile config/gulpfile.js",
    "dev": "cross-env NODE_ENV=development npm run start -- dev",
    "renames": "gulp renames --gulpfile config/gulpfile.js",
    "build": "cross-env NODE_ENV=production npm run start -- build",
    "lint-html": "gulp htmlLint --gulpfile config/gulpfile.js",
    "lint-html:watch": "gulp htmlLintWatch --gulpfile config/gulpfile.js",
    "lint-style": "gulp stylesLint --gulpfile config/gulpfile.js",
    "lint-style:watch": "gulp stylesLintWatch --gulpfile config/gulpfile.js",
    "lint-script": "gulp scriptsLint --gulpfile config/gulpfile.js",
    "lint-script:watch": "gulp scriptsLintWatch --gulpfile config/gulpfile.js",
    "image:optimize": "gulp optimize --gulpfile config/gulpfile.js",
    "commit": "git add . && git-cz",
    "test": "jest",
    "test:tdd": "jest --watch",
    "coverage": "react-app-rewired test --watchAll --roots __tests__ --coverage",
    "release": "generate-release -t"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.10.1",
    "js-cookie": "^2.2.1",
    "mithril": "^2.0.4",
    "prop-types": "^15.7.2",
